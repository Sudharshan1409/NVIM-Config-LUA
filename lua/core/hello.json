{
    "channel": "{{channel_id}}",
    "text": "WebFlow Form Submission",
    "blocks": [
        {
            "type": "header",
            "text": {
                "type": "plain_text",
                "text": "WebFlow Form Submission",
                "emoji": true
            }
        },
        {
            "type": "section",
            "fields": [
                {
                    "type": "mrkdwn",
                    "text": "*First Name:*\n{{firstName}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Last Name:*\n{{lastName}}"
                }
            ]
        },
        {
            "type": "section",
            "fields": [
                {
                    "type": "mrkdwn",
                    "text": "*Email:*\n{{email}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*City:*\n{{city}}"
                }
            ]
        },
        {
            "type": "section",
            "fields": [
                {
                    "type": "mrkdwn",
                    "text": "*State:*\n{{state}}"
                },
                {
                    "type": "mrkdwn",
                    "text": "*Country:*\n{{country}}"
                }
            ]
        },
        {
            "type": "section",
            "fields": [
                {
                    "type": "mrkdwn",
                    "text": "*Contact Number:*\n{{contactNumber}}"
                }
            ]
        }
    ],
    "as_user": true
}
